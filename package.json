{
  "name": "i-ching",
  "version": "0.3.5",
  "description": "library and API for working with the i-ching",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strobus/i-ching.git"
  },
  "author": "p.strobus@yahoo.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strobus/i-ching/issues"
  },
  "homepage": "https://github.com/strobus/i-ching#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "lib/**/*.js",
      "!lib/data.json"
    ],
    "verbose": true
  }
}